  },
  "homepage": "https://github.com/john1234brown/pheonix-box/tree/main/pheonix",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/john1234brown/pheonix-box.git"
  },
  "description": "This is a Typescript Project and NPM Module Along with exported access to the JohnsPheonixBox class object. For usage in NodeJS runtime!",
  "main": "production/pheonix-box.js",
  "types": "types/main.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./production/pheonix-box.d.ts",
        "default": "./production/pheonix-box.js"
      },
      "import": {
        "types": "./production/pheonix-box.mjs.d.ts",
        "default": "./production/pheonix-box.mjs"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:mjs": "tsc --project tsconfig.mjs.json",
    "bundle": "rollup -c",
    "compile": "npm run build && npm run build:mjs && npm run bundle"
  },
  "license": "X11",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^22.7.6",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.6.3"
  }
}